<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.savingdays">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Savingdays">

        <activity
            android:name=".AddProductActivity"
            android:label="@string/title_activity_add_product" />

        <activity
            android:name=".AddScheduleActivity"
            android:label="@string/title_activity_add_schedule" />
        <activity
            android:name=".AddFoodActivity"
            android:label="@string/title_activity_add_food"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".SignUpActivity">
        </activity>
        <activity android:name=".PasswordResetActivity">
        </activity>
        <activity android:name=".CommunityActivity">
        </activity>
        <activity android:name=".MemberInitActivity">
        </activity>
        <activity android:name=".WritePostActivity">
        </activity>
        <activity android:name=".ToDoActivity">
        </activity>
        <activity android:name=".GoogleloginActivity">
        </activity>
        <activity android:name=".ResultActivity">
        </activity>
        <activity android:name=".StoreActivity">
        </activity>
    </application>

</manifest>